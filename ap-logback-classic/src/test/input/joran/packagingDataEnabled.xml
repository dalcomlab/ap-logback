<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration>

<configuration debug="true" packagingData="true">

  <appender name="CONSOLE" class="com.dalcomlab.ap.logback.core.ConsoleAppender">
    <layout class="com.dalcomlab.ap.logback.classic.PatternLayout">
      <param name="Pattern"  value="%d - %m%n"/>
    </layout>
  </appender>

  <root level="DEBUG">
    <appender-ref ref="CONSOLE" />
  </root>

</configuration>

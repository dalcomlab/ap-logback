<?xml version="1.0" encoding="UTF-8"?>

<configuration>

	<appender name="AUTO_BY_SYSTEM_PROPERTY" class="com.dalcomlab.logback.core.ConsoleAppender">
		<layout class="com.dalcomlab.logback.classic.PatternLayout">
			<pattern>%msg%n"</pattern>
		</layout>
	</appender>

	<root>
		<level value="debug" />
		<appender-ref ref="AUTO_BY_SYSTEM_PROPERTY" />
	</root>
</configuration>

<configuration debug="true">

  <appender name="STDOUT" class="com.dalcomlab.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%level - %date - %marker - %msg%n</pattern>
    </encoder>
  </appender>

  <turboFilter class="com.dalcomlab.logback.com.dalcomlab.logback.classic.turbo.MarkerFilter">
    <Marker>ALWAYS</Marker>
    <OnMatch>ACCEPT</OnMatch>
    <OnMismatch>NEUTRAL</OnMismatch>
  </turboFilter>

  <root level="WARN">
    <appender-ref ref="STDOUT"/>
  </root>

</configuration>